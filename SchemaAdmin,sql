CREATE TABLESPACE ADMIN_DATA
    DATAFILE 'D:\app\Daniel\oradata\dbprueba\ADMINdata01.dbf'
    SIZE 10M
    REUSE
    AUTOEXTEND ON
    NEXT 512
    MAXSIZE 200M;   
